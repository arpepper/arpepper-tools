#!/bin/bash

for d in "$@" ; do
	case "$d" in
	(u1004base)
		echo :30
		;;
	(u1404base)
		echo :31
		;;
	(u1404base)
		# problem; we ran three of the same...
		echo :32
		;;
	(u1404base1)
		echo :32
		;;
	(u1404base2)
		echo :33
		;;
	(u1404base3)
		echo :34
		;;
	(u1404mwiki)
		echo :35
		;;
	(u1204base)
		echo :36
		;;
	(u1204gnome)
		echo :37
		;;
	(*)
		echo :40
		;;
	esac
done
